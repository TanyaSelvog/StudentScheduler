<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/email"
        android:layout_width="183dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="37dp"
        android:fontFamily="casual"
        android:letterSpacing="0.2"
        android:text="@string/instructorEmail"
        android:textAllCaps="true"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_number" />

    <TextView
        android:id="@+id/phone_number"
        android:layout_width="183dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="33dp"
        android:fontFamily="casual"
        android:letterSpacing="0.2"
        android:text="@string/instructorPhone"
        android:textAllCaps="true"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/instructor" />

    <TextView
        android:id="@+id/instructor"
        android:layout_width="183dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="132dp"
        android:fontFamily="casual"
        android:letterSpacing="0.2"
        android:text="@string/courseInstructor"
        android:textAllCaps="true"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eDate" />

    <TextView
        android:id="@+id/courseTitle"
        android:layout_width="170dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="casual"
        android:letterSpacing="0.2"
        android:text="@string/courseTitle"
        android:textAllCaps="true"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/eDate"
        android:layout_width="183dp"
        android:layout_height="51dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="52dp"
        android:fontFamily="casual"
        android:text="@string/endDate"
        android:textAllCaps="true"
        android:textSize="16sp"
        android:letterSpacing="0.2"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sDate" />

    <TextView
        android:id="@+id/sDate"
        android:layout_width="179dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="56dp"
        android:fontFamily="casual"
        android:letterSpacing="0.2"
        android:text="@string/startDate"
        android:textAllCaps="true"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/courseTitle" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintStart_toEndOf="@+id/phone_number"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/titleTV"
        android:layout_width="171dp"
        android:layout_height="38dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/courseTitle"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/end_date_TV"
        android:layout_width="185dp"
        android:layout_height="44dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/eDate"
        app:layout_constraintTop_toBottomOf="@+id/sDate" />

    <EditText
        android:id="@+id/start_date_TV"
        android:layout_width="179dp"
        android:layout_height="52dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="60dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/sDate"
        app:layout_constraintTop_toBottomOf="@+id/titleTV" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="148dp"
        android:layout_height="56dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="150dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/instructor"
        app:layout_constraintTop_toBottomOf="@+id/end_date_TV" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintStart_toEndOf="@+id/email"
        app:layout_constraintTop_toBottomOf="@+id/phone_number" />
</androidx.constraintlayout.widget.ConstraintLayout>